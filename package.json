{
  "name": "netden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "prisma generate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "db:generate": "prisma generate",
    "db:migrate:deploy": "prisma migrate deploy",
    "docker:up": "docker compose up --build -d",
    "docker:down": "docker compose down",
    "docker:down:volumes": "docker compose down -v",
    "docker:ps": "docker compose ps",
    "docker:logs": "docker compose logs -f app postgres proxy",
    "docker:smoke": "docker compose up --build -d --wait && curl -fsS http://localhost:${PROXY_PORT:-8080}/api/health && echo \"\\n[smoke] /api/health ok\"",
    "docker:smoke:ephemeral": "docker compose down -v && docker compose up --build -d --wait && curl -fsS http://localhost:${PROXY_PORT:-8080}/api/health && echo \"\\n[smoke] /api/health ok (ephemeral)\""
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.1",
